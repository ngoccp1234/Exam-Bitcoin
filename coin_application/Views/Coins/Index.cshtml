@model IEnumerable<coin_application.Models.Coin>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Market.MarketName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CoinName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BaseAsset)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QuoteAsset)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Volume24h)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedAt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdatedAt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {

  

    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Market.MarketName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoinName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BaseAsset)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QuoteAsset)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Volume24h)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdatedAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CoinId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CoinId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CoinId })
        </td>
    </tr>
}

</table>
